{
  "presets": [
    ["next/babel", {
      "preset-env": {
        "modules": "auto"
      }
    }]
  ],
  "plugins": [
    "@babel/plugin-proposal-function-bind",
    "lodash",
    ["react-intl", {
      "messagesDir": "./build/messages/"
    }],
    ["styled-components", {
      "ssr": true,
      "displayName": true,
      "preprocess": false
    }]
  ],
  "env": {
    "development" : {
      "compact": false
    }
  }
}
