{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-stage-0",
    "next/babel"
  ],
  "plugins": [
    "lodash",
    ["react-intl", {
      "messagesDir": "./build/messages/"
    }],
    ["styled-components", {
      "ssr": true,
      "displayName": true,
      "preprocess": false
    }]
  ],
  "env": {
    "development" : {
      "compact": false
    }
  }
}
